    <section class="main_content dashboard_part large_header_bg">
        <!-- menu  -->

        <!--/ menu  -->
        <div class="main_content_iner overly_inner ">
            <div class="container-fluid p-0 ">
                <!-- page title  -->
                <div class="row">
                    <div class="col-12">
                        <div class="page_title_box d-flex align-items-center justify-content-between">
                            <div class="page_title_left">
                                <h3 class="f_s_30 f_w_700 dark_text">Orders</h3>

                            </div>
                           
                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="col-lg-12">
                        <div class="card QA_section border-0">
                            <div class="card-body QA_table ">
                                <div class="table-responsive shopping-cart ">
                                    <table class="table mb-0">
                                        <thead>
                                            <tr>
                                                <th scope="col">Brand</th>
                                                <th scope="col">Name</th>
                                                <th scope="col">Price</th>
                                                <th scope="col">Image</th>
                                                <th scope="col">Options </th>
                                                </tr>
                                        </thead>
                                       
                                        <tbody>
                                            {{#each product}}
    <tr>
      
      <td>{{this.category}}</td>
      <td>{{this.brandName}}</td>
      <td>{{this.price}}</td>
      <td><img style="width: 25px; height:50px" src="/images/{{this._id}}.jpeg" alt=""></td>
       <td>
        <a href="/admin/edit-section/{{this._id}}" class="btn btn-primary">Edit</a>
        <a href="/admin/delete-product/{{this._id}}" class="btn btn-danger" onclick="return confirm('Are you want to delete {{this.name}}')">Delete</a>
        </td>
    </tr>
    
    {{/each}}


                                        </tbody>
                                       
                                    </table>
                                </div>

                                <!--end row-->
                            </div>
                            <!--end card-->
                        </div>
                        <!--end card-body-->
                    </div>

                    <!--end col-->
                </div>
            </div>
        </div>

